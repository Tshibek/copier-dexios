services:
  docs:
    image: {{project_name}}_local_docs
    container_name: {{project_name}}_local_docs
    build:
      context: .
      dockerfile: ./compose/local/docs/Dockerfile
    env_file:
      - ./.envs/.local/.django
    volumes:
      - ./docs:/docs:z
    ports:
      - '9000:9000'
    command: /start-docs